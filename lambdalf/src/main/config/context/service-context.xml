<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>

<beans>

	<bean id="webscripts.clj.scriptprocessor.factory"
		  class="org.springframework.extensions.webscripts.processor.ClojureScriptProcessorFactory" />

	<bean id="webscripts.clj.scriptprocessor" class="org.springframework.extensions.webscripts.processor.ClojureScriptProcessor">
		<property name="searchPath" ref="webscripts.searchpath"/>
		<property name="scriptProcessorRegistry" ref="webscripts.repo.registry.scriptprocessor" />
	</bean>

	<bean class="org.springframework.extensions.webscripts.ScriptProcessorRegistrar" init-method="init">
      <property name="registry" ref="webscripts.repo.registry.scriptprocessor" />
      <property name="factory" ref="webscripts.clj.scriptprocessor.factory" />
      <property name="name"><value>Clojure Script Processor</value></property>
      <property name="extension"><value>clj</value></property>
   </bean>

	<bean id="swankServer" class="it.sk.alfresco.clojure.RunSwankServer" init-method="init"/>

	<bean id="ContextHelper" class="it.sk.alfresco.clojure.ContextHelper"/>

</beans>
